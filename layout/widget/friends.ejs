<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title"><%- __('Friends') %></h3>
    </div>
    <div class="panel-body">
        <% theme.friends.list.forEach(function(item){ %>
        <% if(item.sidebar==true){ %>
        <a class="frlink" href="<%= item.url %>"><%= item.title %></a><br>
        <% } %>
        <% }) %>
    </div>
</div>