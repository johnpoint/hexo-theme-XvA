<div class="panel panel-disqus">
    <div class="panel-body">
        <div id="disqus_thread">长时间无法加载评论区，请检查您的网络情况</div>
    </div>
</div>
<script>
    var disqusload = function () {
        (function () {
            var d = document, s = d.createElement('script');
            s.src = 'https://<%= theme.comment.disqus.shortname %>.disqus.com/embed.js';
            s.setAttribute('data-timestamp', +new Date());
            (d.head || d.body).appendChild(s);
        })();
    }
    </script>
        <% if (theme.comment.disqus.lazyload) { %>
        <script id="loaddisqus" src="/js/disqus-lazyload.min.js"></script>
        <% } else { %>
        <script>
        disqusload();
        </script>
        <% } %>

<noscript>Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript">comments powered by
        Disqus.</a></noscript>